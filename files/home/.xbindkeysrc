"amixer -c 0 set Master 1dB+"
	XF86AudioRaiseVolume

"amixer -c 0 set Master 1dB-"
	XF86AudioLowerVolume

"(amixer get Master | grep off > /dev/null && amixer -q set Master unmute) || amixer -q set Master mute"
	XF86AudioMute

"xcalib -c"
	XF86MonBrightnessUp

"xcalib -co 95 -a"
	XF86MonBrightnessDown
